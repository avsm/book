(alias
 (name   runtest)
 (deps   (:x README.md)
         (:y3 ../../examples/code/files-modules-and-programs/freq-fast/counter.ml)
         (:y2 ../../examples/code/files-modules-and-programs/freq-fast/counter.mli)
         (:y1 ../../examples/code/maps-and-hash-tables/map_vs_hash/map_vs_hash.ml)
         (:y0 ../../examples/code/maps-and-hash-tables/map_vs_hash2/map_vs_hash2.ml)
         (source_tree ../../examples/code/maps-and-hash-tables/map_vs_hash)
         (source_tree ../../examples/code/maps-and-hash-tables/map_vs_hash2)
         prelude.ml)
 (action (progn
           (run mdx test --prelude=prelude.ml --direction=to-ml %{x})
           (diff? %{x} %{x}.corrected)
           (diff? %{y3} %{y3}.corrected)
           (diff? %{y2} %{y2}.corrected)
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected))))
(alias
 (name   runtest-all)
 (deps   (:x README.md)
         (:y3 ../../examples/code/files-modules-and-programs/freq-fast/counter.ml)
         (:y2 ../../examples/code/files-modules-and-programs/freq-fast/counter.mli)
         (:y1 ../../examples/code/maps-and-hash-tables/map_vs_hash/map_vs_hash.ml)
         (:y0 ../../examples/code/maps-and-hash-tables/map_vs_hash2/map_vs_hash2.ml)
         (source_tree ../../examples/code/maps-and-hash-tables/map_vs_hash)
         (source_tree ../../examples/code/maps-and-hash-tables/map_vs_hash2)
         prelude.ml)
 (action (progn
           (run mdx test --prelude=prelude.ml --direction=to-ml --non-deterministic %{x})
           (diff? %{x} %{x}.corrected)
           (diff? %{y3} %{y3}.corrected)
           (diff? %{y2} %{y2}.corrected)
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected))))
